      }
    },
    "shared-events": {
      "type": "library",
      "root": "libs/shared-events",
      "entryFile": "index",
      "sourceRoot": "libs/shared-events/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared-events/tsconfig.lib.json"
      }
    },
    "shared-queue": {
      "type": "library",
      "root": "libs/shared-queue",
      "entryFile": "index",
      "sourceRoot": "libs/shared-queue/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared-queue/tsconfig.lib.json"
      }
    },
    "shared-rbtmq": {
      "type": "library",
      "root": "libs/shared-rbtmq",
      "entryFile": "index",
      "sourceRoot": "libs/shared-rbtmq/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared-rbtmq/tsconfig.lib.json"
      }
