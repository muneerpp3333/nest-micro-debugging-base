{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api",
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "billing": {
      "type": "application",
      "root": "apps/billing",
      "entryFile": "main",
      "sourceRoot": "apps/billing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/billing/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "entities": {
      "type": "library",
      "root": "libs/entities",
      "entryFile": "index",
      "sourceRoot": "libs/entities/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entities/tsconfig.lib.json"
      }
    },
    "langgraph-client": {
      "type": "library",
      "root": "libs/langgraph-client",
      "entryFile": "index",
      "sourceRoot": "libs/langgraph-client/src",
      "compilerOptions": {
        "tsConfigPath": "libs/langgraph-client/tsconfig.lib.json"
      }
    },
    "listing": {
      "type": "application",
      "root": "apps/listing",
      "entryFile": "main",
      "sourceRoot": "apps/listing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/listing/tsconfig.app.json"
      }
    },
    "messaging": {
      "type": "application",
      "root": "apps/messaging",
      "entryFile": "main",
      "sourceRoot": "apps/messaging/src",
      "compilerOptions": {
        "tsConfigPath": "apps/messaging/tsconfig.app.json"
      }
    },
    "shared-config": {
      "type": "library",
      "root": "libs/shared-config",
      "entryFile": "index",
      "sourceRoot": "libs/shared-config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared-config/tsconfig.lib.json"
      }
    },
    "shared-database": {
      "type": "library",
